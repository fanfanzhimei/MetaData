<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhi.metadata" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data android:name="com.zhi.name" android:value="@string/name"/>
            <meta-data android:name="com.zhi.age" android:value="@string/age"/>
            <meta-data android:name="com.zhi.resourceid" android:resource="@string/hello_world"/>
        </activity>
    </application>

</manifest>
